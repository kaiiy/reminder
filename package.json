{
	"name": "reminder",
	"version": "0.0.0",
	"scripts": {
		"build": "rm -rf dist && node ./bin/build.mjs",
		"lint": "biome lint ./src",
		"lint:fix": "biome check --apply-unsafe ./src",
		"test": "jest --silent=false",
		"format": "biome format --write ./src",
		"check": "npx knip",
		"all": "yarn lint && yarn format && yarn check && yarn test && yarn build"
	},
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.456.0",
		"@line/bot-sdk": "^8.0.2",
		"@tsconfig/recommended": "^1.0.3",
		"@types/aws-lambda": "^8.10.129",
		"@types/node": "20.10.0",
		"aws-lambda": "^1.0.7",
		"console-log-colors": "^0.4.0",
		"date-fns": "^2.30.0",
		"esbuild": "^0.19.7",
		"http-status-codes": "^2.3.0",
		"log-symbols": "^6.0.0",
		"pretty-bytes": "^6.1.1",
		"tinyduration": "^3.3.0",
		"typescript": "^5.3.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.3.3",
		"@types/jest": "^29.5.10",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1"
	},
	"packageManager": "yarn@4.0.2",
	"volta": {
		"node": "20.10.0",
		"yarn": "4.0.2"
	}
}
