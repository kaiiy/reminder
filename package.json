{
	"name": "reminder",
	"version": "0.0.0",
	"scripts": {
		"build": "rm -rf dist && node ./bin/build.mjs",
		"lint": "biome check --apply-unsafe ./src && biome lint ./src",
		"format": "biome format --write ./src",
		"check": "npx knip",
		"all": "yarn lint && yarn format && yarn check && yarn build"
	},
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.451.0",
		"@line/bot-sdk": "^8.0.1",
		"@tsconfig/recommended": "^1.0.3",
		"@types/aws-lambda": "^8.10.126",
		"@types/node": "20.9.1",
		"aws-lambda": "^1.0.7",
		"console-log-colors": "^0.4.0",
		"dayjs": "^1.11.10",
		"esbuild": "^0.19.5",
		"http-status-codes": "^2.3.0",
		"log-symbols": "^6.0.0",
		"pretty-bytes": "^6.1.1",
		"typescript": "^5.2.2",
		"zod": "^3.22.4"
	},
	"packageManager": "yarn@4.0.2",
	"volta": {
		"node": "20.9.0",
		"yarn": "4.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.3.3",
		"knip": "^2.41.3"
	}
}
